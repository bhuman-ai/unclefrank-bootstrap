# Uncle Frank's Bootstrap Environment Variables

# Claude API Key (required)
CLAUDE_API_KEY=your_claude_api_key_here

# Terragon Authentication (required)
TERRAGON_AUTH=your_terragon_auth_token_here

# Task Orchestrator Security (required in production)
ORCHESTRATOR_API_KEY=generate_a_secure_random_key_here

# Optional: Rate limiting configuration
ORCHESTRATOR_RATE_LIMIT_WINDOW_MS=60000
ORCHESTRATOR_RATE_LIMIT_MAX_REQUESTS=30

# Optional: Instance limits
ORCHESTRATOR_MAX_INSTANCES=50
ORCHESTRATOR_MAX_DECISIONS_PER_INSTANCE=10