{
  "name": "unclefrank-bootstrap",
  "version": "0.0.1",
  "description": "Uncle Frank's Autonomous LLM Dev Platform - Bootstrap Core",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "run": "tsx src/index.ts",
    "execute": "node -e \"process.stdin.isTTY = true\" node_modules/.bin/tsx src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "validate": "tsx src/validation/cli.ts validate",
    "validate:watch": "tsx src/validation/cli.ts watch",
    "validate:test": "tsx src/validation/cli.ts test"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "axios": "^1.7.9",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "inquirer": "^11.1.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.17.0",
    "@types/uuid": "^10.0.0",
    "@vercel/node": "^5.3.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": "18.x"
  },
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhuman-ai/unclefrank-bootstrap.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bhuman-ai/unclefrank-bootstrap/issues"
  },
  "homepage": "https://github.com/bhuman-ai/unclefrank-bootstrap#readme"
}
