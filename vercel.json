{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "installCommand": "npm install",
  "functions": {
    "api/task.ts": {
      "runtime": "@vercel/node@3.0.0",
      "includeFiles": "src/**",
      "maxDuration": 30
    }
  },
  "env": {
    "CLAUDE_API_KEY": "@claude-api-key",
    "TERRAGON_AUTH": "@terragon-auth"
  }
}